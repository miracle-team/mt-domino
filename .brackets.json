{
    "path": {
        "scss/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": false,
                "sourceMap": false,
                "outputStyle": "nested"
            },
            "hirse.outline.enabled": false,
            "useTabChar": false,
            "spaceUnits": 2,
            "themes.theme": "monokai-theme",
            "fonts.fontSize": "14px"
        }
    }
}